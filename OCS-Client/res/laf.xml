<synth>



	<!-- window style -->

	<style id="ocsStylePanel">
		<state>
			<opaque value="TRUE" />
			<color value="#444444" type="BACKGROUND" />
			<color value="#ffffff" type="FOREGROUND" />
		</state>
	</style>
	<bind style="ocsStylePanel" type="region" key="Panel" />

	<style id="ocsStyleButton">
		<insets top="0" left="0" right="0" bottom="0" />
		<state value="ENABLED">
			<color value="#ffffff" type="FOREGROUND" />
			<imagePainter method="buttonBackground" path="/Button.png" sourceInsets="10 10 10 10" />
		</state>

		<state value="PRESSED">
			<imagePainter method="buttonBackground" path="/ButtonPressed.png" sourceInsets="10 10 10 10" />
		</state>
	</style>
	<bind style="ocsStyleButton" type="name" key="DefButton" />

	<style id="ocsStyleTextfield">
		<insets top="0" left="10" right="10" bottom="0" />
		<state value="ENABLED">
			<color value="#000000" type="FOREGROUND" />
			<imagePainter method="textfieldBackground" path="/Textfield.png" sourceInsets="10 10 10 10" />
		</state>
	</style>
	<bind style="ocsStyleTextfield" type="name" key="DefTextfield" />

	<style id="ocsStylePasswordfield">
		<insets top="0" left="10" right="10" bottom="0" />
		<state value="ENABLED">
			<color value="#000000" type="FOREGROUND" />
			<imagePainter method="PasswordfieldBackground" path="/Textfield.png" sourceInsets="10 10 10 10" />
		</state>
	</style>
	<bind style="ocsStylePasswordfield" type="name" key="DefPasswordfield" />

	<style id="ocsStyleCheckbox">
		<imageIcon id="check_off" path="/Checkbox_off.png" />
		<imageIcon id="check_on" path="/Checkbox_on.png" />
		<property key="CheckBox.icon" value="check_off" />
		<state value="SELECTED">
			<property key="CheckBox.icon" value="check_on" />
		</state>
	</style>
	<bind style="ocsStyleCheckbox" type="region" key="Checkbox" />
	
	
	
	<!-- tab container -->

	<style id="ocsStyleTab">
		<opaque value="TRUE" />
		<state>
			<insets top="0" left="10" right="10" bottom="5" />
			<font name="Verdana" size="12" />
			<imagePainter method="tabbedPaneTabBackground" path="/Tab.png" sourceInsets="5 5 8 5" />
		</state>

		<state value="SELECTED">
			<insets top="0" left="10" right="10" bottom="5" />
			<font name="Verdana" size="12" />
			<imagePainter method="tabbedPaneTabBackground" path="/TabSelected.png" sourceInsets="5 5 5 5" />
		</state>
	</style>
	<bind style="ocsStyleTab" type="region" key="tabbedPaneTab" />



	<!-- Chat style -->

	<style id="ocsStyleChatButton">
		<insets top="0" left="0" right="0" bottom="0" />
		<state value="ENABLED">
			<color value="#ffffff" type="FOREGROUND" />
			<imagePainter method="buttonBackground" path="/ChatButton.png" sourceInsets="10 10 10 10" />
		</state>

		<state value="PRESSED">
			<imagePainter method="buttonBackground" path="/ChatButtonPressed.png" sourceInsets="10 10 10 10" />
		</state>
	</style>
	<bind style="ocsStyleChatButton" type="name" key="ChatButton" />

	<style id="ocsStyleChatTextfield">
		<insets top="0" left="10" right="10" bottom="0" />
		<state value="ENABLED">
			<color value="#000000" type="FOREGROUND" />
			<imagePainter method="textfieldBackground" path="/ChatTextfield.png" sourceInsets="10 10 10 10" />
		</state>
	</style>
	<bind style="ocsStyleChatTextfield" type="name" key="ChatTextfield" />



	<!-- Party style -->

	<style id="ocsStylePartyJoinButton">
		<insets top="0" left="0" right="0" bottom="0" />
		<state value="ENABLED">
			<color value="#ffffff" type="FOREGROUND" />
			<imagePainter method="buttonBackground" path="/PartyJoinButton.png" sourceInsets="8 8 8 8" />
		</state>

		<state value="PRESSED">
			<imagePainter method="buttonBackground" path="/PartyJoinButtonPressed.png" sourceInsets="8 8 8 8" />
		</state>
	</style>
	<bind style="ocsStylePartyJoinButton" type="name" key="PartyJoinButton" />



	<!-- scrollBarStyle -->

	<style id="ocsStyleScrollBar">
		<state>
			<opaque value="TRUE" />
			<imagePainter method="scrollBarBackground" path="/ScrollBar.png" sourceInsets="1 1 4 4" />
		</state>
	</style>
	<bind style="ocsStyleScrollBar" type="region" key="ScrollBar" />

	<style id="ocsStyleScrollBarThumb">
		<state>
			<imagePainter method="scrollBarThumbBackground" path="/ScrollBarThumb.png" sourceInsets="1 1 1 1" />
		</state>
	</style>
	<bind style="ocsStyleScrollBarThumb" type="region" key="ScrollBarThumb" />

	<style id="ocsStyleScrollBarTrack">
		<state>
			<imagePainter method="scrollBarTrackBackground" path="/ScrollBarTrack.png" sourceInsets="3 3 3 3" />
		</state>
	</style>
	<bind style="ocsStyleScrollBarTrack" type="region" key="ScrollBarTrack" />
</synth>
